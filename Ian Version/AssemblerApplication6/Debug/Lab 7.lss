                                 ;***********************************************************
                                 ;*	Func: led_countdown
                                 ;*	desc: counts down 6 seconds and displays on led's
                                 ;***********************************************************
                                 led_countdown:
0000f7 930f                      push mpr
                                 
0000f8 b105                      in mpr, PORTB
0000f9 6f00                      ori mpr, 0b11110000
0000fa b905                      out PORTB, mpr ;all led's are now set
                                 
0000fb d009                      rcall timer_1_5 ;wait
0000fc 982c                      cbi PORTB, 4 ;clear bit 4
0000fd d007                      rcall timer_1_5 ;repeat for other bits
0000fe 982d                      cbi PORTB, 5
0000ff d005                      rcall timer_1_5
000100 982e                      cbi PORTB, 6
000101 d003                      rcall timer_1_5
000102 982f                      cbi PORTB, 7
                                 
000103 910f                      pop mpr
000104 9508                      ret
                                 ;end led_countdown
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: timer_1_5
                                 ;*	desc: polls timer counter 1 for a 1.5 second timer
                                 ;***********************************************************
                                 timer_1_5:
                                 ;push stuff to stack
000105 930f                      push mpr
                                 
                                 
                                 
000106 e408                      ldi mpr, $48
000107 9300 0085                 sts TCNT1H, mpr
000109 ee04                      ldi mpr, $E4
00010a 9300 0084                 sts TCNT1L, mpr
                                 
                                 timer_1_5_NoFlag:
00010c 9bb0                      sbis TIFR1, 0 ;skip loop if TOV1 is set
00010d cffe                      rjmp timer_1_5_NoFlag
                                 
00010e 9ab0                      sbi TIFR1, 0 ;reset TOV1
                                 
                                 ;pop stuff from stack
                                 
00010f 910f                      pop mpr
000110 9508                      ret
                                 ;end timer_1_5
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_zy_top
                                 ;*	desc: stores string stored in program memory and writes it to the top line of the LCD screen
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator.
                                 ;*	WARNING - assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_top:
000111 930f                      push mpr
000112 93af                      push XL
000113 93bf                      push XH
000114 93ef                      push ZL
000115 93ff                      push ZH
000116 93cf                      push YL
000117 93df                      push YH
                                 
                                 
000118 e0a0                      ldi XL, LOW(lcd_buffer_addr) ;point X to the top line of the LCD buffer address in data memory
000119 e0b1                      ldi XH, HIGH(lcd_buffer_addr)
                                 
                                 print_zy_top_loop:
00011a 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
00011b 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
00011c 17ec                      cp ZL, YL  ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely far enough away to cause roll over errors
00011d f7e1                      brne print_zy_top_loop ;if not at end keep loading LCD buffer
                                 
00011e d0a1                      rcall	LCDWrLn1 ;once done write to LCD
                                 
00011f 91df                      pop YH
000120 91cf                      pop YL
000121 91ff                      pop ZH
000122 91ef                      pop ZL
000123 91bf                      pop XH
000124 91af                      pop XL 
000125 910f                      pop mpr
000126 9508                      ret
                                 ;end print_zy_top
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_zy_bottom
                                 ;*	desc: stores string stored in program memory and writes it to the bottom line of the LCD screen on the left
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	REMEMBER: you must clear line outside of this function to prevent overwriting
                                 ;*	WARNING: assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_bottom:
000127 930f                      push mpr
000128 93af                      push XL
000129 93bf                      push XH
00012a 93ef                      push ZL
00012b 93ff                      push ZH
00012c 93cf                      push YL
00012d 93df                      push YH
                                 
                                 
00012e e1a0                      ldi XL, LOW(lcd_buffer_addr+16) ;point x to the bottom line of the LCD buffer address in data memory
00012f e0b1                      ldi XH, HIGH(lcd_buffer_addr+16)
                                 
                                 print_zy_bottom_left_loop:
000130 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
000131 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
000132 17ec                      cp ZL, YL ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely not far enough away to cause roll over errors
000133 f7e1                      brne print_zy_bottom_left_loop ;if not at end keep loading LCD buffer
                                 
000134 d096                      rcall	LCDWrLn2 ;once done write to LCD 
                                 
000135 91df                      pop YH
000136 91cf                      pop YL
000137 91ff                      pop ZH
000138 91ef                      pop ZL
000139 91bf                      pop XH
00013a 91af                      pop XL 
00013b 910f                      pop mpr
00013c 9508                      ret
                                 ;end print_zy_bottom
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_yz_bottom
                                 ;*	desc: stores string stored in program memory and writes it to the bottom line of the LCD screen on the right
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	REMEMBER: you must clear line outside of this function to prevent overwriting
                                 ;*	WARNING: assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_yz_bottom:
00013d 930f                      push mpr
00013e 93af                      push XL
00013f 93bf                      push XH
000140 93ef                      push ZL
000141 93ff                      push ZH
000142 93cf                      push YL
000143 93df                      push YH
                                 
                                 ;must call lpm on Z, and need to call at end adress so shift Z->Y, and Y->Z for sake of function
000144 2fae                      mov XL, ZL
000145 2fbf                      mov XH, ZH ;X now temporarily holds old Z
                                 
000146 2fec                      mov ZL, YL
000147 2ffd                      mov ZH, YH ;Z now holds old Y 
                                 
000148 2fca                      mov YL, XL
000149 2fdb                      mov YH, XH ;Y now holds old Z via X
                                 
00014a e2a0                      ldi XL, LOW(lcd_buffer_addr+32) ;point x to the bottom line of the LCD buffer address in data memory
00014b e0b1                      ldi XH, HIGH(lcd_buffer_addr+32)
                                 
                                 print_yz_bottom_loop:
00014c 9104                      lpm mpr, Z ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
00014d 930c                      st X, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
00014e 9731                      sbiw ZH:ZL, 1
00014f 9711                      sbiw XH:XL, 1
                                 
                                 
000150 17ec                      cp ZL, YL ;compare where Z points (beginning of string) to Y (Current address), we only need Low byte since start and end are definitely not far enough away to cause roll over errors
000151 f7d1                      brne print_yz_bottom_loop ;if not at end keep loading LCD buffer
                                 
000152 9104                      lpm mpr, Z; store last character (dec YL inside loop triggers reset interrupt for some reason)
000153 930c                      st X, mpr
                                 
000154 d076                      rcall	LCDWrLn2 ;once done write to LCD 
                                 
000155 91df                      pop YH
000156 91cf                      pop YL
000157 91ff                      pop ZH
000158 91ef                      pop ZL
000159 91bf                      pop XH
00015a 91af                      pop XL 
00015b 910f                      pop mpr
00015c 9508                      ret
                                 ;end print_yz_bottom
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
