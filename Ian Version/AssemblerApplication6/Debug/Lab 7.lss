                                 ;*	desc: display welcome screen and poll for PD7
                                 ;***********************************************************
                                 welcome:
000090 930f                      push mpr
                                 
                                 
000091 eee2                      ldi ZL, low(str_welcome1<<1)
000092 e0f2                      ldi ZH, high(str_welcome1<<1)
                                 
000093 eeca                      ldi YL, low(str_welcome1_end<<1)
000094 e0d2                      ldi YH, high(str_welcome1_end<<1)
                                 
000095 d075                      rcall print_zy_top
                                 
000096 eeea                      ldi ZL, low(str_welcome2<<1)
000097 e0f2                      ldi ZH, high(str_welcome2<<1)
                                 
000098 efca                      ldi YL, low(str_welcome2_end<<1)
000099 e0d2                      ldi YH, high(str_welcome2_end<<1)
                                 
00009a d086                      rcall print_zy_bottom
                                 
                                 
                                 welcome_not_pressed:
00009b 994f                      sbic PIND, PD7
00009c cffe                      rjmp welcome_not_pressed
                                 ;PD7 is now pressed
00009d 0000                      nop
00009e 0000                      nop ;avoid some debouncing
                                 welcome_pressed:
00009f 9b4f                      sbis PIND, PD7
0000a0 cffe                      rjmp welcome_pressed
                                 ;PD7 is now released
                                 
                                 
                                 
0000a1 910f                      pop mpr
0000a2 9508                      ret
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: select_choice_left
                                 ;*	desc: cycles through choices for rock paper scissors and prints them to the LCD on the left hand side, preserves right hand side of LCD
                                 ;***********************************************************
                                 select_choice_left:
0000a3 930f                      push mpr
0000a4 932f                      push choice_right
                                 ;changes made to choice left will be saved globally
                                 
                                 ;valid choice values include 1,2,3, for rock paper and scissors respectively. 0 will be initialization value so when button is first pressed rock is shown 
                                 ;check if choice left is 10, load with 00 if so
                                 ;otherwise increment choice left
                                 
0000a5 3013                      cpi choice_left, 3
0000a6 f011                      breq select_choice_left_rollover ;if at two do not increment
                                 
0000a7 9513                      inc choice_left
                                 
0000a8 c001                      rjmp select_choice_left_chosen ;do not roll over if unneeded 
                                 
                                 select_choice_left_rollover:
0000a9 e011                      ldi choice_left, 1
                                 
                                 select_choice_left_chosen:
                                 ;load z and y with labels for str clear
                                 ;call zy print function to write spaces to left hand side of LCD without clearing right hand side
                                 ;based on choice left value load Z and Y with appropriate labels for word
                                 ;call zy print function
                                 
0000aa eaee                      ldi ZL, low(str_clear<<1)
0000ab e0f2                      ldi ZH, high(str_clear<<1)
                                 
0000ac ebc6                      ldi YL, low(str_clear_end<<1)
0000ad e0d2                      ldi YH, high(str_clear_end<<1)
                                 
0000ae d072                      rcall print_zy_bottom ;write clear string to left side of LCD
0000af d013                      rcall load_choice_left ;load Z and Y registers with correct string lables
0000b0 d070                      rcall print_zy_bottom ;print correct choice of string to LCD
                                 
                                 
                                 
                                 
0000b1 912f                      pop choice_right
0000b2 910f                      pop mpr
0000b3 9508                      ret
                                 ;end select_choice_left
                                 
                                 
                                 
                                 
