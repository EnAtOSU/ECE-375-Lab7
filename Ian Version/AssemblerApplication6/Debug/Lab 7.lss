                                 .include "LCDDriver.asm"		; Include the LCD Driver
                                 
                                 ;*	Oregon State University
                                 ;*	ST7565R LCD Driver
                                 ;*	This code configures the graphical LCD and emulates
                                 ;*	the behavior of a 2x16 character display.
                                 ;*
                                 ;*	The LCD has room for 32 characters in total
                                 ;*	(16 on the top and 16 on the bottom).
                                 ;*	The upper 16 characters should be located in SRAM starting at 0x0100.
                                 ;*	The lower 16 characters should be located in SRAM starting at 0x0110.
                                 ;*
                                 ;*	NOTE: Register r16 MUST be named 'mpr' in the Main Code
                                 ;*	Something like this:
                                 ;*	.def	mpr = r16
                                 ;*
                                 ;*	WARNING: Register r20-r22 are reserved and cannot be
                                 ;*		renamed outside of the LCD Driver functions. Doing
                                 ;*		so will damage the functionality of the LCD Driver
                                 ;*
                                 ;*	Acknowledgements: the Bin2ASCII and div10 procedures were written by
                                 ;*	David Zier for a previous board. The code has been retained intact.
                                 ;*
                                 ;*********************************************************************
                                 ;*	Rev	Date		Name			Description
                                 ;*--------------------------------------------------------------------
                                 ;*	-	8/18/22	Justin Goins	Version 1.0
                                 ;*
                                 ;*
                                 ;*********************************************************************
                                 
                                 .include "m32U4def.inc"				; Include definition file
                                 
                                 ;***** Created: 2011-02-09 12:03 ******* Source: ATmega32U4.xml **********
                                 ;*************************************************************************
                                 ;* A P P L I C A T I O N   N O T E   F O R   T H E   A V R   F A M I L Y
                                 ;* 
                                 ;* Number            : AVR000
                                 ;* File Name         : "m32U4def.inc"
                                 ;* Title             : Register/Bit Definitions for the ATmega32U4
                                 ;* Date              : 2011-02-09
                                 ;* Version           : 2.35
                                 ;* Support E-mail    : avr@atmel.com
                                 ;* Target MCU        : ATmega32U4
                                 ;* 
                                 ;* DESCRIPTION
                                 ;* When including this file in the assembly program file, all I/O register 
                                 ;* names and I/O register bit names appearing in the data book can be used.
                                 ;* In addition, the six registers forming the three data pointers X, Y and 
                                 ;* Z have been assigned names XL - ZH. Highest RAM address for Internal 
                                 ;* SRAM is also defined 
                                 ;* 
                                 ;* The Register names are represented by their hexadecimal address.
                                 ;* 
                                 ;* The Register Bit names are represented by their bit number (0-7).
                                 ;* 
                                 ;* Please observe the difference in using the bit names with instructions
                                 ;* such as "sbr"/"cbr" (set/clear bit in register) and "sbrs"/"sbrc"
                                 ;* (skip if bit in register set/cleared). The following example illustrates
                                 ;* this:
                                 ;* 
                                 ;* in    r16,PORTB             ;read PORTB latch
                                 ;* sbr   r16,(1<<PB6)+(1<<PB5) ;set PB6 and PB5 (use masks, not bit#)
                                 ;* out   PORTB,r16             ;output to PORTB
                                 ;* 
                                 ;* in    r16,TIFR              ;read the Timer Interrupt Flag Register
                                 ;* sbrc  r16,TOV0              ;test the overflow flag (use bit#)
                                 ;* rjmp  TOV0_is_set           ;jump if set
                                 ;* ...                         ;otherwise do something else
                                 ;*************************************************************************
                                 
                                 #ifndef _M32U4DEF_INC_
                                 #endif  /* _M32U4DEF_INC_ */
                                 
                                 ; ***** END OF FILE ******************************************************
                                 
                                 ; Register definitions
                                 .def	count = r20			; Character Counter
                                 .def	q = r21			; Quotient for div10
                                 .def	r = r22			; Remander for div10
                                 
                                 ; The following constants are not intended to be modified.
                                 .equ 	lcd_buffer_addr = 0x0100 ; define the SRAM base address of the LCD buffer
                                 
                                 ; This information is gathered from the ST7565R datasheet.
                                 ; define some command patterns
                                 .equ	lcd_c_disp_en = 0b10101110 ; lsb indicates whether the LCD is on (1) or off (0)
                                 .equ	lcd_c_disp_set_start_ln = 0b01000000 ; lower 6 bits indicate the line index (0 - 63)
                                 .equ	lcd_c_disp_set_page_addr = 0b10110000 ; lower 4 bits indicate the page address (page 3 is at top and page 0 is at bottom)
                                 ; together, these commands allow you to specify a column index from (0 - 131):
                                 .equ	lcd_c_disp_set_col_addr_h = 0b00010000 ; lower 4 bits indicate the 4 high bits of the column address
                                 .equ	lcd_c_disp_set_col_addr_l = 0b00000000 ; lower 4 bits indicate the 4 low bits of the column address
                                 .equ	lcd_c_disp_set_invert = 0b10100110 ; lsb indicates whether the LCD is normal (0) or inverted (1)
                                 .equ	lcd_c_disp_fill = 0b10100100 ; lsb indicates whether the LCD is normal (0) or filled with dots (1)
                                 .equ	lcd_c_disp_reset = 0b11100010 ; resets the line/page addresses and other stuff, does not affect data RAM
                                 .equ	lcd_c_disp_set_bias = 0xA2 ; 1/6 bias (in 1/33 duty mode)
                                 .equ	lcd_c_disp_set_scandir = 0b11000000 ; select normal scan direction (most significant bit is placed at top of LCD)
                                 .equ	lcd_c_disp_set_contrast_1 = 0b10000001 ; first we send this command then we pass the value
                                 .equ	lcd_c_disp_set_contrast_2 = 15 ; can select a contrast value between 1 and 63
                                 .equ	lcd_c_disp_set_res_ratio = 0x22 ; see the datasheet
                                 .equ	lcd_c_disp_vreg_on = 0x2F ; enable voltage booster, voltage regulator, and voltage follower
                                 
                                 ; use this command to write into the LCD RAM:
                                 .equ	lcd_c_disp_write = 0b00000000 ; these 8 bits are the data to be placed in the target. 
                                 ; note that the column address is automatically incremented by 1 after each write to the LCD data
                                 
                                 ; everything in this driver file needs to go into the code segment
                                 .cseg
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDInit
                                 ;* Desc:	This function initializes the LCD control pins and
                                 ;*		sets reasonable values for the configuration registers.
                                 ;*****************************************************************
                                 LCDInit:
0001a8 930f                      	push	mpr
                                 
                                 	; set MOSI, SCL, and SS as outputs
0001a9 9a22                      	sbi	DDRB, DDB2	; Port B 2
0001aa 9a21                      	sbi	DDRB, DDB1	; Port B 1
0001ab 9a20                      	sbi	DDRB, DDB0	; Port B 0
                                 	; ensure that SS is deactivated (active low)
0001ac 9a28                      	sbi	PORTB, 0
                                 	; set lcd_A0 as output
0001ad 9a81                      	sbi	DDRF, DDF1	; Port F 1
                                 	; enable LCD backlight control
0001ae 9a3f                      	sbi	DDRC, DDC7	; Port C 7
                                 	; set lcd_RST_N as output
0001af 9a80                      	sbi	DDRF, DDF0	; Port F 0
                                 	; we pull lcd_RST_N low for 1 millisecond
0001b0 9888                      	cbi	PORTF, 0
0001b1 940e 0293                 	call	LCDDelay
0001b3 9a88                      	sbi	PORTF, 0
                                 	; Enable SPI mode 3, master configuration, set clock rate fck/2 using SPI2X
0001b4 e50c                      	ldi	mpr, (1<<SPE)|(1<<MSTR)|(1<<CPOL)|(1<<CPHA)
0001b5 bd0c                      	out	SPCR, mpr
0001b6 e001                      	ldi	mpr, (1<<SPI2X)
0001b7 bd0d                      	out	SPSR, mpr
                                 	; Note: after transmitting a byte, the SPI interrupt flag will
                                 	; be automatically cleared next time we: 1) read SPSR and ALSO 2) access SPDR
                                 
0001b8 9828                      	cbi	PORTB, 0	; activate slave select
                                 	; send a string of initialization commands
0001b9 ea02                      	ldi	mpr, lcd_c_disp_set_bias
0001ba d0d2                      	rcall	LCD_Internal_WriteCMD
0001bb ec00                      	ldi	mpr, lcd_c_disp_set_scandir
0001bc d0d0                      	rcall	LCD_Internal_WriteCMD
0001bd e801                      	ldi	mpr, lcd_c_disp_set_contrast_1
0001be d0ce                      	rcall	LCD_Internal_WriteCMD
0001bf e00f                      	ldi	mpr, lcd_c_disp_set_contrast_2
0001c0 d0cc                      	rcall	LCD_Internal_WriteCMD
0001c1 e202                      	ldi	mpr, lcd_c_disp_set_res_ratio
0001c2 d0ca                      	rcall	LCD_Internal_WriteCMD
0001c3 e20f                      	ldi	mpr, lcd_c_disp_vreg_on
0001c4 d0c8                      	rcall	LCD_Internal_WriteCMD
0001c5 ea0f                      	ldi	mpr, (lcd_c_disp_en | 1 << 0)
0001c6 d0c6                      	rcall	LCD_Internal_WriteCMD
0001c7 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
0001c8 910f                      	pop	mpr
0001c9 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDBacklightOn
                                 ;* Desc:	Enables the backlight
                                 ;*****************************************************************
                                 LCDBacklightOn:
0001ca 9a47                      	sbi	PORTC, 7
0001cb 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDBacklightOff
                                 ;* Desc:	Disables the backlight
                                 ;*****************************************************************
                                 LCDBacklightOff:
0001cc 9847                      	cbi	PORTC, 7
0001cd 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrite
                                 ;* Desc:	This function writes 32 characters to the LCD screen...
                                 ;*		16 chars on the top line and 16 chars on the bottom line.
                                 ;*****************************************************************
                                 LCDWrite:
                                 	
0001ce d002                      	rcall	LCDWrLn1
0001cf d00c                      	rcall	LCDWrLn2
0001d0 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn1
                                 ;* Desc:	This function writes 16 characters to the top line.
                                 ;*****************************************************************
                                 LCDWrLn1:
0001d1 93bf                      	push	XH
0001d2 93af                      	push	XL
0001d3 931f                      	push 	r17
                                 	; r17 specifies the destination bank in LCD RAM
0001d4 e013                      	ldi	r17, 3
                                 	; point X at the ASCII data
0001d5 e0b1                      	ldi	XH, HIGH(lcd_buffer_addr)
0001d6 e0a0                      	ldi	XL, LOW(lcd_buffer_addr)
0001d7 d05c                      	rcall	LCD_Internal_WriteLn
0001d8 911f                      	pop	r17
0001d9 91af                      	pop	XL
0001da 91bf                      	pop	XH
0001db 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn2
                                 ;* Desc:	This function writes 16 characters to the bottom line.
                                 ;*****************************************************************
                                 LCDWrLn2:
0001dc 93bf                      	push	XH
0001dd 93af                      	push	XL
0001de 931f                      	push	 r17
                                 	; r17 specifies the destination bank in LCD RAM
0001df e011                      	ldi	r17, 1
                                 	; point X at the ASCII data
0001e0 e0b1                      	ldi	XH, HIGH(lcd_buffer_addr+16)
0001e1 e1a0                      	ldi	XL, LOW(lcd_buffer_addr+16)
0001e2 d051                      	rcall	LCD_Internal_WriteLn
0001e3 911f                      	pop	r17
0001e4 91af                      	pop	XL
0001e5 91bf                      	pop	XH
0001e6 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDClr
                                 ;* Desc:	This function clears the entire LCD screen.
                                 ;*****************************************************************
                                 LCDClr:
0001e7 d022                      	rcall	LCDClrLn1
0001e8 d027                      	rcall	LCDClrLn2
                                 
0001e9 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	CLRDM1
                                 ;* Desc:	This function clears Data Memory by writing 16 
                                 ;*		consecutive ASCII spaces $20 to the memory at $0100.
                                 ;*****************************************************************
                                 CLRDM1:
                                     ; Set X register points to $0100
0001ea 93bf                      	   push	   XH
0001eb 93af                      	   PUSH	   XL
0001ec 930f                                 push    r16
0001ed 931f                                 push    r17
                                 
0001ee e0b1                                 ldi     XH, HIGH(lcd_buffer_addr)
0001ef e0a0                                 ldi     XL, LOW(lcd_buffer_addr)
0001f0 e110                                 ldi     r17, 16
0001f1 e200                                 ldi     r16, ' '
                                    CLRloop1:
0001f2 930d                                 st      X+, r16         ; load data to DM
0001f3 951a                                 dec     r17
0001f4 f7e9                                 brne    CLRloop1
                                 
0001f5 911f                      	   pop	   r17
0001f6 910f                      	   pop	   r16
0001f7 91af                      	   pop	   XL
0001f8 91bf                      	   pop	   XH
                                 
0001f9 9508                                 ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	CLRDM2
                                 ;* Desc:	This function clears Data Memory by writing 16 
                                 ;*		consecutive ASCII spaces $20 to the memory at $0110.
                                 ;*****************************************************************
                                 CLRDM2:
                                     ; Set X register points to $0110
0001fa 93bf                      	   push	   XH
0001fb 93af                      	   PUSH	   XL
0001fc 930f                                 push    r16
0001fd 931f                                 push    r17
                                 
0001fe e0b1                                 ldi     XH, HIGH(lcd_buffer_addr+16)
0001ff e1a0                                 ldi     XL, LOW(lcd_buffer_addr+16)
000200 e110                                 ldi     r17, 16
000201 e200                                 ldi     r16, ' '
                                    CLRloop2:
000202 930d                                 st      X+, r16         ; load data to DM
000203 951a                                 dec     r17
000204 f7e9                                 brne    CLRloop2
                                 
000205 911f                      	   pop	   r17
000206 910f                      	   pop	   r16
000207 91af                      	   pop	   XL
000208 91bf                      	   pop	   XH
                                 
000209 9508                                 ret
                                 ;*****************************************************************
                                 ;* Func:	LCDClrLn1
                                 ;* Desc:	This function clears the top line of the LCD screen.
                                 ;*****************************************************************
                                 LCDClrLn1:
00020a 931f                      	push	 r17
                                 	; r17 specifies the target bank in LCD RAM
00020b e013                      	ldi	r17, 3
00020c d060                      	rcall	LCD_Internal_ClearLn
00020d dfdc                      	rcall	CLRDM1
00020e 911f                      	pop	r17
00020f 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDClrLn2
                                 ;* Desc:	This function clears the bottom line of the LCD screen.
                                 ;*****************************************************************
                                 LCDClrLn2:
000210 931f                      	push	 r17
                                 	; r17 specifies the target bank in LCD RAM
000211 e011                      	ldi	r17, 1
000212 d05a                      	rcall	LCD_Internal_ClearLn
000213 dfe6                      	rcall	CLRDM2
000214 911f                      	pop	r17
000215 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*****************************************************************
                                 Bin2ASCII:
000216 930f                      	push	 mpr			; save mpr
000217 936f                      	push	 r			; save r
000218 935f                      	push	 q			; save q
000219 93bf                      	push	 XH			; save X-pointer
00021a 93af                      	push	 XL			;
                                 
                                 	; Determine the range of mpr
00021b 3604                      	cpi	mpr, 100		; is mpr >= 100
00021c f018                      	brlo	 B2A_1			; goto next check
00021d e043                      	ldi	count, 3		; Three chars are written
00021e 9613                      	adiw	 XL, 3			; Increment X 3 address spaces
00021f c007                      	rjmp	 B2A_3			; Continue with program
                                 B2A_1:
000220 300a                      	cpi	mpr, 10		; is mpr >= 10
000221 f018                      	brlo	 B2A_2			; Continue with program
000222 e042                      	ldi	count, 2		; Two chars are written
000223 9612                      	adiw	 XL, 2			; Increment X 2 address spaces
000224 c002                      	rjmp	 B2A_3			; Continue with program
                                 B2A_2:
000225 9611                      	adiw	 XL, 1			; Increment X 1 address space
000226 e041                      	ldi	count, 1		; One char is written
                                 
                                 B2A_3:
                                 	; Do-While statement that converts Binary to ASCII
000227 d076                      	rcall	div10			; Call the div10 function
000228 e300                      	ldi	mpr, '0'		; Set the base ASCII integer value
000229 0f06                      	add	mpr, r		; Create the ASCII integer value
00022a 930e                      	st	-X, mpr		; Load ASCII value to memory
00022b 2f05                      	mov	mpr, q		; Set mpr to quotient value
00022c 3000                      	cpi	mpr, 0		; does mpr == 0
00022d f7c9                      	brne 	B2A_3			; do while (mpr != 0)
                                 
00022e 91af                      	pop	XL			; restore X-pointer
00022f 91bf                      	pop	XH			;
000230 915f                      	pop	q			; restore q
000231 916f                      	pop	r			; restore r
000232 910f                      	pop	mpr			; restore mpr
000233 9508                      	ret				; return from function
                                 
                                 ;*****************************************************************
                                 ;*	The functions below here are intended for internal use only.
                                 ;*****************************************************************
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteLn
                                 ;* Desc:	Sends 16 characters of information to the LCD panel
                                 ;*		This is slightly more complicated than it sounds because the
                                 ;*		characters straddle two banks of the LCD memory.
                                 ;*		X is pointing at the first ASCII character to be displayed.
                                 ;*		r17 holds the index of the destination bank in LCD RAM.
                                 ;*		
                                 ;*****************************************************************
                                 LCD_Internal_WriteLn:
000234 920f                      	push	 r0
000235 921f                      	push	 r1
000236 930f                      	push	 mpr
000237 932f                      	push	 r18
000238 933f                      	push	 r19
000239 938f                      	push	 r24
00023a 939f                      	push	 r25
00023b 93ef                      	push	 ZL
00023c 93ff                      	push	 ZH
                                 
                                 	; r25:24 holds a pointer to the base address of the bitmap array
00023d e095                      	ldi	r25, HIGH(FontBitmap << 1);
00023e e888                      	ldi	r24, LOW(FontBitmap << 1);
                                 
00023f 9828                      	cbi	PORTB, 0	; activate slave select
                                 
                                 	; use a counter to track if we are on the top chunk or bottom chunk of each char
000240 e022                      	ldi	r18, 2
                                 
                                 LCD_WL_display_bank:
                                 	; set the column to 0
000241 e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
000242 d04a                      	rcall	LCD_Internal_WriteCMD
000243 e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
000244 d048                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
000245 eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
000246 2b01                      	or	mpr, r17	; set the lower 4 bits
000247 d045                      	rcall	LCD_Internal_WriteCMD
                                 
000248 e130                      	ldi	r19, 16	; there are 16 characters per line
                                 
                                 LCD_WL_next_char:
                                 	; get ascii code point & determine location of corresponding character bitmap
000249 900d                      	ld	r0, X+	; this should be a value from 0 to 255
00024a e100                      	ldi	mpr, 16	; there are 16 bytes per character
00024b 9e00                      	mul	r0, mpr	; determine the byte offset for the desired char
00024c 01fc                      	movw	 ZH:ZL, r25:r24	; copy the base address to Z
00024d 0de0                      	add	ZL, r0	; finally, compute a pointer to the char bitmap
00024e 1df1                      	adc	ZH, r1
                                 
00024f e008                      	ldi	mpr, 8	; there are 8 columns for each character
                                 
                                 	; this is where we display the chunk of character (either a top chunk or bottom chunk)
000250 9004                      	lpm	r0, Z
                                 LCD_WL_next_col:
000251 bc0e                      	out	SPDR, r0
000252 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
000253 9632                      	adiw	 ZH:ZL, 2	; point Z at the next column for this bank
000254 9004                      	lpm	r0, Z		; this is a sneaky way to save a few clock cycles (since we are waiting for the SPI communication)
                                 LCD_WL_wait_spi:
000255 b41d                      	in	r1, SPSR
000256 fe17                      	sbrs	 r1, SPIF	; wait for SPI to finish
000257 cffd                      	rjmp	 LCD_WL_wait_spi
                                 
                                 	; keep going until we have displayed all the columns for this character
000258 950a                      	dec	mpr
000259 f7b9                      	brne	 LCD_WL_next_col
                                 
                                 	; move to the next character
00025a 953a                      	dec	r19
00025b f769                      	brne	 LCD_WL_next_char
                                 
                                 	; if we are at the end of the line, configure the bank and starting array offset for the bottom half of each char
00025c 952a                      	dec	r18		; did we display both banks yet?
00025d f021                      	breq	 LCD_WL_all_done
                                 	; if not, then prepare to display the bottom chunk of each character
00025e 951a                      	dec	r17		; decrement the bank number
00025f 9750                      	sbiw	 XH:XL, 16	; X needs to point back at the beginning character of the line
000260 9601                      	adiw	 r25:r24, 1	; increment the base address pointer (now it will point to the second chunk of each char)
000261 cfdf                      	rjmp	 LCD_WL_display_bank
                                 
                                 LCD_WL_all_done:
000262 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
000263 91ff                      	pop	ZH
000264 91ef                      	pop	ZL
000265 919f                      	pop	r25
000266 918f                      	pop	r24
000267 913f                      	pop	r19
000268 912f                      	pop	r18
000269 910f                      	pop	mpr
00026a 901f                      	pop	r1
00026b 900f                      	pop	r0
00026c 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_ClearLn
                                 ;* Desc:	This function clears two of the LCD memory banks.
                                 ;*		r17 specifies the starting bank to wipe.
                                 ;*****************************************************************
                                 LCD_Internal_ClearLn:
00026d 930f                      	push 	mpr
00026e 921f                      	push 	r1
00026f 932f                      	push	 r18
000270 933f                      	push	 r19
                                 
000271 9828                      	cbi	PORTB, 0	; activate slave select
000272 e032                      	ldi	r19, 2	; we are going to clear two banks
                                 
                                 LCD_CLRLN_clear_bank:
                                 	; set the column to 0
000273 e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
000274 d018                      	rcall	LCD_Internal_WriteCMD
000275 e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
000276 d016                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
000277 eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
000278 2b01                      	or	mpr, r17	; configure the lower 4 bits
000279 d013                      	rcall	LCD_Internal_WriteCMD
                                 
00027a e820                      	ldi	r18, 128	; there are 128 columns on the LCD
00027b 2700                      	clr	mpr		; we will fill each column with empty dots (i.e. blank space)
00027c 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
                                 
                                 LCD_CLRLN_write_col:
00027d bd0e                      	out	SPDR, mpr	; send an empty column to the LCD via SPI
                                 LCD_CLRLN_wait_spi:
00027e b41d                      	in	r1, SPSR
00027f fe17                      	sbrs	 r1, SPIF	; wait for SPI to finish
000280 cffd                      	rjmp 	LCD_CLRLN_wait_spi
                                 
000281 952a                      	dec	r18
000282 f7d1                      	brne	LCD_CLRLN_write_col	; cycle through all 128 columns
                                 
                                 	; clear the remaining bank if we haven't done so already
000283 953a                      	dec	r19
000284 f011                      	breq	LCD_CLRLN_all_done
000285 951a                      	dec	r17		; decrement the bank index
000286 cfec                      	rjmp	LCD_CLRLN_clear_bank
                                 
                                 LCD_CLRLN_all_done:
000287 9a28                      	sbi	PORTB, 0	; deactivate slave select	
                                 	
                                 	; pop registers...
000288 913f                      	pop	r19
000289 912f                      	pop	r18
00028a 901f                      	pop	r1
00028b 910f                      	pop	mpr
00028c 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteCMD
                                 ;* Desc:	Sends the byte in mpr
                                 ;*		If you use this function directly,
                                 ;*		YOU ARE RESPONSIBLE for handling the slave select line!
                                 ;*****************************************************************
                                 LCD_Internal_WriteCMD:
00028d bd0e                      	out	SPDR, mpr	; initiate transmission
00028e 9889                      	cbi	PORTF, 1	; clear lcd_A0 to put the lcd into command mode
                                 				; (this just has to be done before the 8th bit
                                 				; is transmitted via SPI)
                                 LCD_IWC_L1:
00028f b50d                      	in	mpr, SPSR
000290 ff07                      	sbrs	 mpr, SPIF	; wait for SPI to finish
000291 cffd                      	rjmp LCD_IWC_L1
000292 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDDelay
                                 ;* Desc:	A wait loop that will consume roughly 1ms
                                 ;*		Note that a CALL takes 4 cycles on top of this delay.
                                 ;*****************************************************************
                                 LCDDelay:
000293 930f                      	push 	mpr		; 2 cycles
000294 931f                      	push	 r17		; 2 cycles
000295 e01b                      	ldi	r17, 11	; 1 cycle
                                 LCDDelay_L1:
000296 ef02                      	ldi	mpr, 242	; 1 cycle
                                 LCDDelay_L2:
000297 950a                      	dec	mpr		; 1 cycle
000298 f7f1                      	brne	LCDDelay_L2	; 2 cycles on branch, 1 if no branch
000299 951a                      	dec	r17		; 1 cycle
00029a f7d9                      	brne	LCDDelay_L1	; 2 cycles on branch, 1 if no branch
00029b 911f                      	pop	r17		; 2 cycles
00029c 910f                      	pop	mpr		; 2 cycles
00029d 9508                      	ret			; 4 cycles
                                 
                                 ;*****************************************************************
                                 ;* Func:	div10
                                 ;* Desc:	Divides the value in the mpr by 10 and 
                                 ;*			puts the remainder in the 'r' register
                                 ;*			and the quotient in the 'q' register.
                                 ;*	DO NOT modify this function, trust me, it does
                                 ;*	divide by 10 :)  ~DZ		
                                 ;*****************************************************************
                                 div10:
00029e 920f                      	push	r0		; Save register
                                 
                                 	; q = mpr / 10 = mpr * 0.000110011001101b
00029f 2f50                      	mov	q, mpr	; q = mpr * 1.0b
0002a0 9556                      	lsr	q		; q >> 2
0002a1 9556                      	lsr	q		; q = mpr * 0.01b
0002a2 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0002a3 9556                      	lsr	q		; q = mpr * 0.101b
0002a4 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0002a5 9556                      	lsr	q
0002a6 9556                      	lsr	q
0002a7 9556                      	lsr	q		; q = mpr * 0.001101b
0002a8 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0002a9 9556                      	lsr	q		; q = mpr * 0.1001101b
0002aa 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0002ab 9556                      	lsr	q				
0002ac 9556                      	lsr	q
0002ad 9556                      	lsr	q		; q = mpr * 0.0011001101b
0002ae 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0002af 9556                      	lsr	q		; q = mpr * 0.10011001101b
0002b0 0f50                      	add	q, mpr	; q = (q + mpr) >> 4
0002b1 9556                      	lsr	q
0002b2 9556                      	lsr	q
0002b3 9556                      	lsr	q
0002b4 9556                      	lsr	q		; q = mpr * 0.000110011001101b
                                 
                                 	; compute the remainder as r = i - 10 * q
                                 	; calculate r = q * 10 = q * 1010b
0002b5 2f65                      	mov	r, q		; r = q * 1
0002b6 0f66                      	lsl	r		; r << 2
0002b7 0f66                      	lsl	r		; r = q * 100b
0002b8 0f65                      	add	r, q		; r = (r + q) << 1
0002b9 0f66                      	lsl	r		; r = q * 1010b
0002ba 2e06                      	mov	r0, r		; r0 = 10 * q
0002bb 2f60                      	mov	r, mpr	; r = mpr
0002bc 1960                      	sub	r, r0		; r = mpr - 10 * q
                                 
                                 	; Fix any errors that occur
                                 div10_1:
0002bd 306a                      	cpi	r, 10		; Compare with 10
0002be f018                      	brlo	 div10_2	; do nothing if r < 10
0002bf 9553                      	inc	q		; fix quotient
0002c0 506a                      	subi	r, 10		; fix remainder
0002c1 cffb                      	rjmp	 div10_1	; Continue until error is corrected
                                 
                                 div10_2:
0002c2 900f                      	pop	r0		; Restore registers
0002c3 9508                      	ret			; Return from function
                                 
