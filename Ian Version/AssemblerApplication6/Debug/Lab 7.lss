                                 welcome:
0000a7 930f                      push mpr
                                 
                                 
0000a8 e1e0                      ldi ZL, low(str_welcome1<<1)
0000a9 e0f3                      ldi ZH, high(str_welcome1<<1)
                                 
0000aa e1c8                      ldi YL, low(str_welcome1_end<<1)
0000ab e0d3                      ldi YH, high(str_welcome1_end<<1)
                                 
