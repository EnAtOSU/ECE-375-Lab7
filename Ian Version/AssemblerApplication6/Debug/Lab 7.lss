                                 ;***********************************************************
                                 ;*	Func: led_countdown
                                 ;*	desc: counts down 6 seconds and displays on led's
                                 ;***********************************************************
                                 led_countdown:
000108 930f                      push mpr
                                 
000109 b105                      in mpr, PORTB
00010a 6f00                      ori mpr, 0b11110000
00010b b905                      out PORTB, mpr ;all led's are now set
                                 
00010c d009                      rcall timer_1_5 ;wait
00010d 982c                      cbi PORTB, 4 ;clear bit 4
00010e d007                      rcall timer_1_5 ;repeat for other bits
00010f 982d                      cbi PORTB, 5
000110 d005                      rcall timer_1_5
000111 982e                      cbi PORTB, 6
000112 d003                      rcall timer_1_5
000113 982f                      cbi PORTB, 7
                                 
000114 910f                      pop mpr
000115 9508                      ret
                                 ;end led_countdown
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: timer_1_5
                                 ;*	desc: polls timer counter 1 for a 1.5 second timer
                                 ;***********************************************************
                                 timer_1_5:
                                 ;push stuff to stack
000116 930f                      push mpr
                                 
                                 
                                 
000117 e408                      ldi mpr, $48
000118 9300 0085                 sts TCNT1H, mpr
00011a ee04                      ldi mpr, $E4
00011b 9300 0084                 sts TCNT1L, mpr
                                 
                                 timer_1_5_NoFlag:
00011d 9bb0                      sbis TIFR1, 0 ;skip loop if TOV1 is set
00011e cffe                      rjmp timer_1_5_NoFlag
                                 
00011f 9ab0                      sbi TIFR1, 0 ;reset TOV1
                                 
                                 ;pop stuff from stack
                                 
000120 910f                      pop mpr
000121 9508                      ret
                                 ;end timer_1_5
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_zy_top
                                 ;*	desc: stores string stored in program memory and writes it to the top line of the LCD screen
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator.
                                 ;*	WARNING - assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_top:
000122 930f                      push mpr
000123 93af                      push XL
000124 93bf                      push XH
000125 93ef                      push ZL
000126 93ff                      push ZH
000127 93cf                      push YL
000128 93df                      push YH
                                 
                                 
000129 e0a0                      ldi XL, LOW(lcd_buffer_addr) ;point X to the top line of the LCD buffer address in data memory
00012a e0b1                      ldi XH, HIGH(lcd_buffer_addr)
                                 
                                 print_zy_top_loop:
00012b 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
00012c 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
00012d 17ec                      cp ZL, YL  ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely far enough away to cause roll over errors
00012e f7e1                      brne print_zy_top_loop ;if not at end keep loading LCD buffer
                                 
00012f d0a1                      rcall	LCDWrLn1 ;once done write to LCD
                                 
000130 91df                      pop YH
000131 91cf                      pop YL
000132 91ff                      pop ZH
000133 91ef                      pop ZL
000134 91bf                      pop XH
000135 91af                      pop XL 
000136 910f                      pop mpr
000137 9508                      ret
                                 ;end print_zy_top
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_zy_bottom
                                 ;*	desc: stores string stored in program memory and writes it to the bottom line of the LCD screen on the left
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	REMEMBER: you must clear line outside of this function to prevent overwriting
                                 ;*	WARNING: assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_bottom:
000138 930f                      push mpr
000139 93af                      push XL
00013a 93bf                      push XH
00013b 93ef                      push ZL
00013c 93ff                      push ZH
00013d 93cf                      push YL
00013e 93df                      push YH
                                 
                                 
00013f e1a0                      ldi XL, LOW(lcd_buffer_addr+16) ;point x to the bottom line of the LCD buffer address in data memory
000140 e0b1                      ldi XH, HIGH(lcd_buffer_addr+16)
                                 
                                 print_zy_bottom_left_loop:
000141 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
000142 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
000143 17ec                      cp ZL, YL ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely not far enough away to cause roll over errors
000144 f7e1                      brne print_zy_bottom_left_loop ;if not at end keep loading LCD buffer
                                 
000145 d096                      rcall	LCDWrLn2 ;once done write to LCD 
                                 
000146 91df                      pop YH
000147 91cf                      pop YL
000148 91ff                      pop ZH
000149 91ef                      pop ZL
00014a 91bf                      pop XH
00014b 91af                      pop XL 
00014c 910f                      pop mpr
00014d 9508                      ret
                                 ;end print_zy_bottom
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_yz_bottom
                                 ;*	desc: stores string stored in program memory and writes it to the bottom line of the LCD screen on the right
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	REMEMBER: you must clear line outside of this function to prevent overwriting
                                 ;*	WARNING: assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_yz_bottom:
00014e 930f                      push mpr
00014f 93af                      push XL
000150 93bf                      push XH
000151 93ef                      push ZL
000152 93ff                      push ZH
000153 93cf                      push YL
000154 93df                      push YH
                                 
                                 ;must call lpm on Z, and need to call at end adress so shift Z->Y, and Y->Z for sake of function
000155 2fae                      mov XL, ZL
000156 2fbf                      mov XH, ZH ;X now temporarily holds old Z
                                 
000157 2fec                      mov ZL, YL
000158 2ffd                      mov ZH, YH ;Z now holds old Y 
                                 
000159 2fca                      mov YL, XL
00015a 2fdb                      mov YH, XH ;Y now holds old Z via X
                                 
00015b e2a0                      ldi XL, LOW(lcd_buffer_addr+32) ;point x to the bottom line of the LCD buffer address in data memory
00015c e0b1                      ldi XH, HIGH(lcd_buffer_addr+32)
                                 
                                 print_yz_bottom_loop:
00015d 9104                      lpm mpr, Z ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
00015e 930c                      st X, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
00015f 9731                      sbiw ZH:ZL, 1
000160 9711                      sbiw XH:XL, 1
                                 
                                 
000161 17ec                      cp ZL, YL ;compare where Z points (beginning of string) to Y (Current address), we only need Low byte since start and end are definitely not far enough away to cause roll over errors
000162 f7d1                      brne print_yz_bottom_loop ;if not at end keep loading LCD buffer
                                 
000163 9104                      lpm mpr, Z; store last character (dec YL inside loop triggers reset interrupt for some reason)
000164 930c                      st X, mpr
                                 
000165 d076                      rcall	LCDWrLn2 ;once done write to LCD 
                                 
000166 91df                      pop YH
000167 91cf                      pop YL
000168 91ff                      pop ZH
000169 91ef                      pop ZL
00016a 91bf                      pop XH
00016b 91af                      pop XL 
00016c 910f                      pop mpr
00016d 9508                      ret
                                 ;end print_yz_bottom
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
