                                 .include "LCDDriver.asm"		; Include the LCD Driver
                                 
                                 ;*	Oregon State University
                                 ;*	ST7565R LCD Driver
                                 ;*	This code configures the graphical LCD and emulates
                                 ;*	the behavior of a 2x16 character display.
                                 ;*
                                 ;*	The LCD has room for 32 characters in total
                                 ;*	(16 on the top and 16 on the bottom).
                                 ;*	The upper 16 characters should be located in SRAM starting at 0x0100.
                                 ;*	The lower 16 characters should be located in SRAM starting at 0x0110.
                                 ;*
                                 ;*	NOTE: Register r16 MUST be named 'mpr' in the Main Code
                                 ;*	Something like this:
                                 ;*	.def	mpr = r16
                                 ;*
                                 ;*	WARNING: Register r20-r22 are reserved and cannot be
                                 ;*		renamed outside of the LCD Driver functions. Doing
                                 ;*		so will damage the functionality of the LCD Driver
                                 ;*
                                 ;*	Acknowledgements: the Bin2ASCII and div10 procedures were written by
                                 ;*	David Zier for a previous board. The code has been retained intact.
                                 ;*
                                 ;*********************************************************************
                                 ;*	Rev	Date		Name			Description
                                 ;*--------------------------------------------------------------------
                                 ;*	-	8/18/22	Justin Goins	Version 1.0
                                 ;*
                                 ;*
                                 ;*********************************************************************
                                 
                                 .include "m32U4def.inc"				; Include definition file
                                 
                                 ;***** Created: 2011-02-09 12:03 ******* Source: ATmega32U4.xml **********
                                 ;*************************************************************************
                                 ;* A P P L I C A T I O N   N O T E   F O R   T H E   A V R   F A M I L Y
                                 ;* 
                                 ;* Number            : AVR000
                                 ;* File Name         : "m32U4def.inc"
                                 ;* Title             : Register/Bit Definitions for the ATmega32U4
                                 ;* Date              : 2011-02-09
                                 ;* Version           : 2.35
                                 ;* Support E-mail    : avr@atmel.com
                                 ;* Target MCU        : ATmega32U4
                                 ;* 
                                 ;* DESCRIPTION
                                 ;* When including this file in the assembly program file, all I/O register 
                                 ;* names and I/O register bit names appearing in the data book can be used.
                                 ;* In addition, the six registers forming the three data pointers X, Y and 
                                 ;* Z have been assigned names XL - ZH. Highest RAM address for Internal 
                                 ;* SRAM is also defined 
                                 ;* 
                                 ;* The Register names are represented by their hexadecimal address.
                                 ;* 
                                 ;* The Register Bit names are represented by their bit number (0-7).
                                 ;* 
                                 ;* Please observe the difference in using the bit names with instructions
                                 ;* such as "sbr"/"cbr" (set/clear bit in register) and "sbrs"/"sbrc"
                                 ;* (skip if bit in register set/cleared). The following example illustrates
                                 ;* this:
                                 ;* 
                                 ;* in    r16,PORTB             ;read PORTB latch
                                 ;* sbr   r16,(1<<PB6)+(1<<PB5) ;set PB6 and PB5 (use masks, not bit#)
                                 ;* out   PORTB,r16             ;output to PORTB
                                 ;* 
                                 ;* in    r16,TIFR              ;read the Timer Interrupt Flag Register
                                 ;* sbrc  r16,TOV0              ;test the overflow flag (use bit#)
                                 ;* rjmp  TOV0_is_set           ;jump if set
                                 ;* ...                         ;otherwise do something else
                                 ;*************************************************************************
                                 
                                 #ifndef _M32U4DEF_INC_
                                 #endif  /* _M32U4DEF_INC_ */
                                 
                                 ; ***** END OF FILE ******************************************************
                                 
                                 ; Register definitions
                                 .def	count = r20			; Character Counter
                                 .def	q = r21			; Quotient for div10
                                 .def	r = r22			; Remander for div10
                                 
                                 ; The following constants are not intended to be modified.
                                 .equ 	lcd_buffer_addr = 0x0100 ; define the SRAM base address of the LCD buffer
                                 
                                 ; This information is gathered from the ST7565R datasheet.
                                 ; define some command patterns
                                 .equ	lcd_c_disp_en = 0b10101110 ; lsb indicates whether the LCD is on (1) or off (0)
                                 .equ	lcd_c_disp_set_start_ln = 0b01000000 ; lower 6 bits indicate the line index (0 - 63)
                                 .equ	lcd_c_disp_set_page_addr = 0b10110000 ; lower 4 bits indicate the page address (page 3 is at top and page 0 is at bottom)
                                 ; together, these commands allow you to specify a column index from (0 - 131):
                                 .equ	lcd_c_disp_set_col_addr_h = 0b00010000 ; lower 4 bits indicate the 4 high bits of the column address
                                 .equ	lcd_c_disp_set_col_addr_l = 0b00000000 ; lower 4 bits indicate the 4 low bits of the column address
                                 .equ	lcd_c_disp_set_invert = 0b10100110 ; lsb indicates whether the LCD is normal (0) or inverted (1)
                                 .equ	lcd_c_disp_fill = 0b10100100 ; lsb indicates whether the LCD is normal (0) or filled with dots (1)
                                 .equ	lcd_c_disp_reset = 0b11100010 ; resets the line/page addresses and other stuff, does not affect data RAM
                                 .equ	lcd_c_disp_set_bias = 0xA2 ; 1/6 bias (in 1/33 duty mode)
                                 .equ	lcd_c_disp_set_scandir = 0b11000000 ; select normal scan direction (most significant bit is placed at top of LCD)
                                 .equ	lcd_c_disp_set_contrast_1 = 0b10000001 ; first we send this command then we pass the value
                                 .equ	lcd_c_disp_set_contrast_2 = 15 ; can select a contrast value between 1 and 63
                                 .equ	lcd_c_disp_set_res_ratio = 0x22 ; see the datasheet
                                 .equ	lcd_c_disp_vreg_on = 0x2F ; enable voltage booster, voltage regulator, and voltage follower
                                 
                                 ; use this command to write into the LCD RAM:
                                 .equ	lcd_c_disp_write = 0b00000000 ; these 8 bits are the data to be placed in the target. 
                                 ; note that the column address is automatically incremented by 1 after each write to the LCD data
                                 
                                 ; everything in this driver file needs to go into the code segment
                                 .cseg
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDInit
                                 ;* Desc:	This function initializes the LCD control pins and
                                 ;*		sets reasonable values for the configuration registers.
                                 ;*****************************************************************
                                 LCDInit:
000191 930f                      	push	mpr
                                 
                                 	; set MOSI, SCL, and SS as outputs
000192 9a22                      	sbi	DDRB, DDB2	; Port B 2
000193 9a21                      	sbi	DDRB, DDB1	; Port B 1
000194 9a20                      	sbi	DDRB, DDB0	; Port B 0
                                 	; ensure that SS is deactivated (active low)
000195 9a28                      	sbi	PORTB, 0
                                 	; set lcd_A0 as output
000196 9a81                      	sbi	DDRF, DDF1	; Port F 1
                                 	; enable LCD backlight control
000197 9a3f                      	sbi	DDRC, DDC7	; Port C 7
                                 	; set lcd_RST_N as output
000198 9a80                      	sbi	DDRF, DDF0	; Port F 0
                                 	; we pull lcd_RST_N low for 1 millisecond
000199 9888                      	cbi	PORTF, 0
00019a 940e 027c                 	call	LCDDelay
00019c 9a88                      	sbi	PORTF, 0
                                 	; Enable SPI mode 3, master configuration, set clock rate fck/2 using SPI2X
00019d e50c                      	ldi	mpr, (1<<SPE)|(1<<MSTR)|(1<<CPOL)|(1<<CPHA)
00019e bd0c                      	out	SPCR, mpr
00019f e001                      	ldi	mpr, (1<<SPI2X)
0001a0 bd0d                      	out	SPSR, mpr
                                 	; Note: after transmitting a byte, the SPI interrupt flag will
                                 	; be automatically cleared next time we: 1) read SPSR and ALSO 2) access SPDR
                                 
0001a1 9828                      	cbi	PORTB, 0	; activate slave select
                                 	; send a string of initialization commands
0001a2 ea02                      	ldi	mpr, lcd_c_disp_set_bias
0001a3 d0d2                      	rcall	LCD_Internal_WriteCMD
0001a4 ec00                      	ldi	mpr, lcd_c_disp_set_scandir
0001a5 d0d0                      	rcall	LCD_Internal_WriteCMD
0001a6 e801                      	ldi	mpr, lcd_c_disp_set_contrast_1
0001a7 d0ce                      	rcall	LCD_Internal_WriteCMD
0001a8 e00f                      	ldi	mpr, lcd_c_disp_set_contrast_2
0001a9 d0cc                      	rcall	LCD_Internal_WriteCMD
0001aa e202                      	ldi	mpr, lcd_c_disp_set_res_ratio
0001ab d0ca                      	rcall	LCD_Internal_WriteCMD
0001ac e20f                      	ldi	mpr, lcd_c_disp_vreg_on
0001ad d0c8                      	rcall	LCD_Internal_WriteCMD
0001ae ea0f                      	ldi	mpr, (lcd_c_disp_en | 1 << 0)
0001af d0c6                      	rcall	LCD_Internal_WriteCMD
0001b0 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
0001b1 910f                      	pop	mpr
0001b2 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDBacklightOn
                                 ;* Desc:	Enables the backlight
                                 ;*****************************************************************
                                 LCDBacklightOn:
0001b3 9a47                      	sbi	PORTC, 7
0001b4 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDBacklightOff
                                 ;* Desc:	Disables the backlight
                                 ;*****************************************************************
                                 LCDBacklightOff:
0001b5 9847                      	cbi	PORTC, 7
0001b6 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrite
                                 ;* Desc:	This function writes 32 characters to the LCD screen...
                                 ;*		16 chars on the top line and 16 chars on the bottom line.
                                 ;*****************************************************************
                                 LCDWrite:
                                 	
0001b7 d002                      	rcall	LCDWrLn1
0001b8 d00c                      	rcall	LCDWrLn2
0001b9 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn1
                                 ;* Desc:	This function writes 16 characters to the top line.
                                 ;*****************************************************************
                                 LCDWrLn1:
0001ba 93bf                      	push	XH
0001bb 93af                      	push	XL
0001bc 931f                      	push 	r17
                                 	; r17 specifies the destination bank in LCD RAM
0001bd e013                      	ldi	r17, 3
                                 	; point X at the ASCII data
0001be e0b1                      	ldi	XH, HIGH(lcd_buffer_addr)
0001bf e0a0                      	ldi	XL, LOW(lcd_buffer_addr)
0001c0 d05c                      	rcall	LCD_Internal_WriteLn
0001c1 911f                      	pop	r17
0001c2 91af                      	pop	XL
0001c3 91bf                      	pop	XH
0001c4 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn2
                                 ;* Desc:	This function writes 16 characters to the bottom line.
                                 ;*****************************************************************
                                 LCDWrLn2:
0001c5 93bf                      	push	XH
0001c6 93af                      	push	XL
0001c7 931f                      	push	 r17
                                 	; r17 specifies the destination bank in LCD RAM
0001c8 e011                      	ldi	r17, 1
                                 	; point X at the ASCII data
0001c9 e0b1                      	ldi	XH, HIGH(lcd_buffer_addr+16)
0001ca e1a0                      	ldi	XL, LOW(lcd_buffer_addr+16)
0001cb d051                      	rcall	LCD_Internal_WriteLn
0001cc 911f                      	pop	r17
0001cd 91af                      	pop	XL
0001ce 91bf                      	pop	XH
0001cf 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDClr
                                 ;* Desc:	This function clears the entire LCD screen.
                                 ;*****************************************************************
                                 LCDClr:
0001d0 d022                      	rcall	LCDClrLn1
0001d1 d027                      	rcall	LCDClrLn2
                                 
0001d2 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	CLRDM1
                                 ;* Desc:	This function clears Data Memory by writing 16 
                                 ;*		consecutive ASCII spaces $20 to the memory at $0100.
                                 ;*****************************************************************
                                 CLRDM1:
                                     ; Set X register points to $0100
0001d3 93bf                      	   push	   XH
0001d4 93af                      	   PUSH	   XL
0001d5 930f                                 push    r16
0001d6 931f                                 push    r17
                                 
0001d7 e0b1                                 ldi     XH, HIGH(lcd_buffer_addr)
0001d8 e0a0                                 ldi     XL, LOW(lcd_buffer_addr)
0001d9 e110                                 ldi     r17, 16
0001da e200                                 ldi     r16, ' '
                                    CLRloop1:
0001db 930d                                 st      X+, r16         ; load data to DM
0001dc 951a                                 dec     r17
0001dd f7e9                                 brne    CLRloop1
                                 
0001de 911f                      	   pop	   r17
0001df 910f                      	   pop	   r16
0001e0 91af                      	   pop	   XL
0001e1 91bf                      	   pop	   XH
                                 
0001e2 9508                                 ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	CLRDM2
                                 ;* Desc:	This function clears Data Memory by writing 16 
                                 ;*		consecutive ASCII spaces $20 to the memory at $0110.
                                 ;*****************************************************************
                                 CLRDM2:
                                     ; Set X register points to $0110
0001e3 93bf                      	   push	   XH
0001e4 93af                      	   PUSH	   XL
0001e5 930f                                 push    r16
0001e6 931f                                 push    r17
                                 
0001e7 e0b1                                 ldi     XH, HIGH(lcd_buffer_addr+16)
0001e8 e1a0                                 ldi     XL, LOW(lcd_buffer_addr+16)
0001e9 e110                                 ldi     r17, 16
0001ea e200                                 ldi     r16, ' '
                                    CLRloop2:
0001eb 930d                                 st      X+, r16         ; load data to DM
0001ec 951a                                 dec     r17
0001ed f7e9                                 brne    CLRloop2
                                 
0001ee 911f                      	   pop	   r17
0001ef 910f                      	   pop	   r16
0001f0 91af                      	   pop	   XL
0001f1 91bf                      	   pop	   XH
                                 
0001f2 9508                                 ret
                                 ;*****************************************************************
                                 ;* Func:	LCDClrLn1
                                 ;* Desc:	This function clears the top line of the LCD screen.
                                 ;*****************************************************************
                                 LCDClrLn1:
0001f3 931f                      	push	 r17
                                 	; r17 specifies the target bank in LCD RAM
0001f4 e013                      	ldi	r17, 3
0001f5 d060                      	rcall	LCD_Internal_ClearLn
0001f6 dfdc                      	rcall	CLRDM1
0001f7 911f                      	pop	r17
0001f8 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDClrLn2
                                 ;* Desc:	This function clears the bottom line of the LCD screen.
                                 ;*****************************************************************
                                 LCDClrLn2:
0001f9 931f                      	push	 r17
                                 	; r17 specifies the target bank in LCD RAM
0001fa e011                      	ldi	r17, 1
0001fb d05a                      	rcall	LCD_Internal_ClearLn
0001fc dfe6                      	rcall	CLRDM2
0001fd 911f                      	pop	r17
0001fe 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*****************************************************************
                                 Bin2ASCII:
0001ff 930f                      	push	 mpr			; save mpr
000200 936f                      	push	 r			; save r
000201 935f                      	push	 q			; save q
000202 93bf                      	push	 XH			; save X-pointer
000203 93af                      	push	 XL			;
                                 
                                 	; Determine the range of mpr
000204 3604                      	cpi	mpr, 100		; is mpr >= 100
000205 f018                      	brlo	 B2A_1			; goto next check
000206 e043                      	ldi	count, 3		; Three chars are written
000207 9613                      	adiw	 XL, 3			; Increment X 3 address spaces
000208 c007                      	rjmp	 B2A_3			; Continue with program
                                 B2A_1:
000209 300a                      	cpi	mpr, 10		; is mpr >= 10
00020a f018                      	brlo	 B2A_2			; Continue with program
00020b e042                      	ldi	count, 2		; Two chars are written
00020c 9612                      	adiw	 XL, 2			; Increment X 2 address spaces
00020d c002                      	rjmp	 B2A_3			; Continue with program
                                 B2A_2:
00020e 9611                      	adiw	 XL, 1			; Increment X 1 address space
00020f e041                      	ldi	count, 1		; One char is written
                                 
                                 B2A_3:
                                 	; Do-While statement that converts Binary to ASCII
000210 d076                      	rcall	div10			; Call the div10 function
000211 e300                      	ldi	mpr, '0'		; Set the base ASCII integer value
000212 0f06                      	add	mpr, r		; Create the ASCII integer value
000213 930e                      	st	-X, mpr		; Load ASCII value to memory
000214 2f05                      	mov	mpr, q		; Set mpr to quotient value
000215 3000                      	cpi	mpr, 0		; does mpr == 0
000216 f7c9                      	brne 	B2A_3			; do while (mpr != 0)
                                 
000217 91af                      	pop	XL			; restore X-pointer
000218 91bf                      	pop	XH			;
000219 915f                      	pop	q			; restore q
00021a 916f                      	pop	r			; restore r
00021b 910f                      	pop	mpr			; restore mpr
00021c 9508                      	ret				; return from function
                                 
                                 ;*****************************************************************
                                 ;*	The functions below here are intended for internal use only.
                                 ;*****************************************************************
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteLn
                                 ;* Desc:	Sends 16 characters of information to the LCD panel
                                 ;*		This is slightly more complicated than it sounds because the
                                 ;*		characters straddle two banks of the LCD memory.
                                 ;*		X is pointing at the first ASCII character to be displayed.
                                 ;*		r17 holds the index of the destination bank in LCD RAM.
                                 ;*		
                                 ;*****************************************************************
                                 LCD_Internal_WriteLn:
00021d 920f                      	push	 r0
00021e 921f                      	push	 r1
00021f 930f                      	push	 mpr
000220 932f                      	push	 r18
000221 933f                      	push	 r19
000222 938f                      	push	 r24
000223 939f                      	push	 r25
000224 93ef                      	push	 ZL
000225 93ff                      	push	 ZH
                                 
                                 	; r25:24 holds a pointer to the base address of the bitmap array
000226 e095                      	ldi	r25, HIGH(FontBitmap << 1);
000227 e58a                      	ldi	r24, LOW(FontBitmap << 1);
                                 
000228 9828                      	cbi	PORTB, 0	; activate slave select
                                 
                                 	; use a counter to track if we are on the top chunk or bottom chunk of each char
000229 e022                      	ldi	r18, 2
                                 
                                 LCD_WL_display_bank:
                                 	; set the column to 0
00022a e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
00022b d04a                      	rcall	LCD_Internal_WriteCMD
00022c e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
00022d d048                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
00022e eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
00022f 2b01                      	or	mpr, r17	; set the lower 4 bits
000230 d045                      	rcall	LCD_Internal_WriteCMD
                                 
000231 e130                      	ldi	r19, 16	; there are 16 characters per line
                                 
                                 LCD_WL_next_char:
                                 	; get ascii code point & determine location of corresponding character bitmap
000232 900d                      	ld	r0, X+	; this should be a value from 0 to 255
000233 e100                      	ldi	mpr, 16	; there are 16 bytes per character
000234 9e00                      	mul	r0, mpr	; determine the byte offset for the desired char
000235 01fc                      	movw	 ZH:ZL, r25:r24	; copy the base address to Z
000236 0de0                      	add	ZL, r0	; finally, compute a pointer to the char bitmap
000237 1df1                      	adc	ZH, r1
                                 
000238 e008                      	ldi	mpr, 8	; there are 8 columns for each character
                                 
                                 	; this is where we display the chunk of character (either a top chunk or bottom chunk)
000239 9004                      	lpm	r0, Z
                                 LCD_WL_next_col:
00023a bc0e                      	out	SPDR, r0
00023b 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
00023c 9632                      	adiw	 ZH:ZL, 2	; point Z at the next column for this bank
00023d 9004                      	lpm	r0, Z		; this is a sneaky way to save a few clock cycles (since we are waiting for the SPI communication)
                                 LCD_WL_wait_spi:
00023e b41d                      	in	r1, SPSR
00023f fe17                      	sbrs	 r1, SPIF	; wait for SPI to finish
000240 cffd                      	rjmp	 LCD_WL_wait_spi
                                 
                                 	; keep going until we have displayed all the columns for this character
000241 950a                      	dec	mpr
000242 f7b9                      	brne	 LCD_WL_next_col
                                 
                                 	; move to the next character
000243 953a                      	dec	r19
000244 f769                      	brne	 LCD_WL_next_char
                                 
                                 	; if we are at the end of the line, configure the bank and starting array offset for the bottom half of each char
000245 952a                      	dec	r18		; did we display both banks yet?
000246 f021                      	breq	 LCD_WL_all_done
                                 	; if not, then prepare to display the bottom chunk of each character
000247 951a                      	dec	r17		; decrement the bank number
000248 9750                      	sbiw	 XH:XL, 16	; X needs to point back at the beginning character of the line
000249 9601                      	adiw	 r25:r24, 1	; increment the base address pointer (now it will point to the second chunk of each char)
00024a cfdf                      	rjmp	 LCD_WL_display_bank
                                 
                                 LCD_WL_all_done:
00024b 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
00024c 91ff                      	pop	ZH
00024d 91ef                      	pop	ZL
00024e 919f                      	pop	r25
00024f 918f                      	pop	r24
000250 913f                      	pop	r19
000251 912f                      	pop	r18
000252 910f                      	pop	mpr
000253 901f                      	pop	r1
000254 900f                      	pop	r0
000255 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_ClearLn
                                 ;* Desc:	This function clears two of the LCD memory banks.
                                 ;*		r17 specifies the starting bank to wipe.
                                 ;*****************************************************************
                                 LCD_Internal_ClearLn:
000256 930f                      	push 	mpr
000257 921f                      	push 	r1
000258 932f                      	push	 r18
000259 933f                      	push	 r19
                                 
00025a 9828                      	cbi	PORTB, 0	; activate slave select
00025b e032                      	ldi	r19, 2	; we are going to clear two banks
                                 
                                 LCD_CLRLN_clear_bank:
                                 	; set the column to 0
00025c e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
00025d d018                      	rcall	LCD_Internal_WriteCMD
00025e e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
00025f d016                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
000260 eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
000261 2b01                      	or	mpr, r17	; configure the lower 4 bits
000262 d013                      	rcall	LCD_Internal_WriteCMD
                                 
000263 e820                      	ldi	r18, 128	; there are 128 columns on the LCD
000264 2700                      	clr	mpr		; we will fill each column with empty dots (i.e. blank space)
000265 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
                                 
                                 LCD_CLRLN_write_col:
000266 bd0e                      	out	SPDR, mpr	; send an empty column to the LCD via SPI
                                 LCD_CLRLN_wait_spi:
000267 b41d                      	in	r1, SPSR
000268 fe17                      	sbrs	 r1, SPIF	; wait for SPI to finish
000269 cffd                      	rjmp 	LCD_CLRLN_wait_spi
                                 
00026a 952a                      	dec	r18
00026b f7d1                      	brne	LCD_CLRLN_write_col	; cycle through all 128 columns
                                 
                                 	; clear the remaining bank if we haven't done so already
00026c 953a                      	dec	r19
00026d f011                      	breq	LCD_CLRLN_all_done
00026e 951a                      	dec	r17		; decrement the bank index
00026f cfec                      	rjmp	LCD_CLRLN_clear_bank
                                 
                                 LCD_CLRLN_all_done:
000270 9a28                      	sbi	PORTB, 0	; deactivate slave select	
                                 	
                                 	; pop registers...
000271 913f                      	pop	r19
000272 912f                      	pop	r18
000273 901f                      	pop	r1
000274 910f                      	pop	mpr
000275 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteCMD
                                 ;* Desc:	Sends the byte in mpr
                                 ;*		If you use this function directly,
                                 ;*		YOU ARE RESPONSIBLE for handling the slave select line!
                                 ;*****************************************************************
                                 LCD_Internal_WriteCMD:
000276 bd0e                      	out	SPDR, mpr	; initiate transmission
000277 9889                      	cbi	PORTF, 1	; clear lcd_A0 to put the lcd into command mode
                                 				; (this just has to be done before the 8th bit
                                 				; is transmitted via SPI)
                                 LCD_IWC_L1:
000278 b50d                      	in	mpr, SPSR
000279 ff07                      	sbrs	 mpr, SPIF	; wait for SPI to finish
00027a cffd                      	rjmp LCD_IWC_L1
00027b 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDDelay
                                 ;* Desc:	A wait loop that will consume roughly 1ms
                                 ;*		Note that a CALL takes 4 cycles on top of this delay.
                                 ;*****************************************************************
                                 LCDDelay:
00027c 930f                      	push 	mpr		; 2 cycles
00027d 931f                      	push	 r17		; 2 cycles
00027e e01b                      	ldi	r17, 11	; 1 cycle
                                 LCDDelay_L1:
00027f ef02                      	ldi	mpr, 242	; 1 cycle
                                 LCDDelay_L2:
000280 950a                      	dec	mpr		; 1 cycle
000281 f7f1                      	brne	LCDDelay_L2	; 2 cycles on branch, 1 if no branch
000282 951a                      	dec	r17		; 1 cycle
000283 f7d9                      	brne	LCDDelay_L1	; 2 cycles on branch, 1 if no branch
000284 911f                      	pop	r17		; 2 cycles
000285 910f                      	pop	mpr		; 2 cycles
000286 9508                      	ret			; 4 cycles
                                 
                                 ;*****************************************************************
                                 ;* Func:	div10
                                 ;* Desc:	Divides the value in the mpr by 10 and 
                                 ;*			puts the remainder in the 'r' register
                                 ;*			and the quotient in the 'q' register.
                                 ;*	DO NOT modify this function, trust me, it does
                                 ;*	divide by 10 :)  ~DZ		
                                 ;*****************************************************************
                                 div10:
000287 920f                      	push	r0		; Save register
                                 
                                 	; q = mpr / 10 = mpr * 0.000110011001101b
000288 2f50                      	mov	q, mpr	; q = mpr * 1.0b
000289 9556                      	lsr	q		; q >> 2
00028a 9556                      	lsr	q		; q = mpr * 0.01b
00028b 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
00028c 9556                      	lsr	q		; q = mpr * 0.101b
00028d 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
00028e 9556                      	lsr	q
00028f 9556                      	lsr	q
000290 9556                      	lsr	q		; q = mpr * 0.001101b
000291 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
000292 9556                      	lsr	q		; q = mpr * 0.1001101b
000293 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
000294 9556                      	lsr	q				
000295 9556                      	lsr	q
000296 9556                      	lsr	q		; q = mpr * 0.0011001101b
000297 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
000298 9556                      	lsr	q		; q = mpr * 0.10011001101b
000299 0f50                      	add	q, mpr	; q = (q + mpr) >> 4
00029a 9556                      	lsr	q
00029b 9556                      	lsr	q
00029c 9556                      	lsr	q
00029d 9556                      	lsr	q		; q = mpr * 0.000110011001101b
                                 
                                 	; compute the remainder as r = i - 10 * q
                                 	; calculate r = q * 10 = q * 1010b
00029e 2f65                      	mov	r, q		; r = q * 1
00029f 0f66                      	lsl	r		; r << 2
0002a0 0f66                      	lsl	r		; r = q * 100b
0002a1 0f65                      	add	r, q		; r = (r + q) << 1
0002a2 0f66                      	lsl	r		; r = q * 1010b
0002a3 2e06                      	mov	r0, r		; r0 = 10 * q
0002a4 2f60                      	mov	r, mpr	; r = mpr
0002a5 1960                      	sub	r, r0		; r = mpr - 10 * q
                                 
                                 	; Fix any errors that occur
                                 div10_1:
0002a6 306a                      	cpi	r, 10		; Compare with 10
0002a7 f018                      	brlo	 div10_2	; do nothing if r < 10
0002a8 9553                      	inc	q		; fix quotient
0002a9 506a                      	subi	r, 10		; fix remainder
0002aa cffb                      	rjmp	 div10_1	; Continue until error is corrected
                                 
                                 div10_2:
0002ab 900f                      	pop	r0		; Restore registers
0002ac 9508                      	ret			; Return from function
                                 
