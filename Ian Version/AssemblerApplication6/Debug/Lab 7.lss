                                 ;***********************************************************
                                 ;*	Func: led_countdown
                                 ;*	desc: counts down 6 seconds and displays on led's
                                 ;***********************************************************
                                 led_countdown:
0000f1 930f                      push mpr
                                 
0000f2 b105                      in mpr, PORTB
0000f3 6f00                      ori mpr, 0b11110000
0000f4 b905                      out PORTB, mpr ;all led's are now set
                                 
0000f5 d009                      rcall timer_1_5 ;wait
0000f6 982c                      cbi PORTB, 4 ;clear bit 4
0000f7 d007                      rcall timer_1_5 ;repeat for other bits
0000f8 982d                      cbi PORTB, 5
0000f9 d005                      rcall timer_1_5
0000fa 982e                      cbi PORTB, 6
0000fb d003                      rcall timer_1_5
0000fc 982f                      cbi PORTB, 7
                                 
0000fd 910f                      pop mpr
0000fe 9508                      ret
                                 ;end led_countdown
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: timer_1_5
                                 ;*	desc: polls timer counter 1 for a 1.5 second timer
                                 ;***********************************************************
                                 timer_1_5:
                                 ;push stuff to stack
0000ff 930f                      push mpr
                                 
                                 
                                 
000100 e408                      ldi mpr, $48
000101 9300 0085                 sts TCNT1H, mpr
000103 ee04                      ldi mpr, $E4
000104 9300 0084                 sts TCNT1L, mpr
                                 
                                 timer_1_5_NoFlag:
000106 9bb0                      sbis TIFR1, 0 ;skip loop if TOV1 is set
000107 cffe                      rjmp timer_1_5_NoFlag
                                 
000108 9ab0                      sbi TIFR1, 0 ;reset TOV1
                                 
                                 ;pop stuff from stack
                                 
000109 910f                      pop mpr
00010a 9508                      ret
                                 ;end timer_1_5
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_zy_top
                                 ;*	desc: stores string stored in program memory and writes it to the top line of the LCD screen
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator.
                                 ;*	WARNING - assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_top:
00010b 930f                      push mpr
00010c 93af                      push XL
00010d 93bf                      push XH
00010e 93ef                      push ZL
00010f 93ff                      push ZH
000110 93cf                      push YL
000111 93df                      push YH
                                 
                                 
000112 e0a0                      ldi XL, LOW(lcd_buffer_addr) ;point X to the top line of the LCD buffer address in data memory
000113 e0b1                      ldi XH, HIGH(lcd_buffer_addr)
                                 
                                 print_zy_top_loop:
000114 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
000115 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
000116 17ec                      cp ZL, YL  ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely far enough away to cause roll over errors
000117 f7e1                      brne print_zy_top_loop ;if not at end keep loading LCD buffer
                                 
000118 d0a1                      rcall	LCDWrLn1 ;once done write to LCD
                                 
000119 91df                      pop YH
00011a 91cf                      pop YL
00011b 91ff                      pop ZH
00011c 91ef                      pop ZL
00011d 91bf                      pop XH
00011e 91af                      pop XL 
00011f 910f                      pop mpr
000120 9508                      ret
                                 ;end print_zy_top
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_zy_bottom
                                 ;*	desc: stores string stored in program memory and writes it to the bottom line of the LCD screen on the left
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	REMEMBER: you must clear line outside of this function to prevent overwriting
                                 ;*	WARNING: assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_bottom:
000121 930f                      push mpr
000122 93af                      push XL
000123 93bf                      push XH
000124 93ef                      push ZL
000125 93ff                      push ZH
000126 93cf                      push YL
000127 93df                      push YH
                                 
                                 
000128 e1a0                      ldi XL, LOW(lcd_buffer_addr+16) ;point x to the bottom line of the LCD buffer address in data memory
000129 e0b1                      ldi XH, HIGH(lcd_buffer_addr+16)
                                 
                                 print_zy_bottom_left_loop:
00012a 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
00012b 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
00012c 17ec                      cp ZL, YL ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely not far enough away to cause roll over errors
00012d f7e1                      brne print_zy_bottom_left_loop ;if not at end keep loading LCD buffer
                                 
00012e d096                      rcall	LCDWrLn2 ;once done write to LCD 
                                 
00012f 91df                      pop YH
000130 91cf                      pop YL
000131 91ff                      pop ZH
000132 91ef                      pop ZL
000133 91bf                      pop XH
000134 91af                      pop XL 
000135 910f                      pop mpr
000136 9508                      ret
                                 ;end print_zy_bottom
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_yz_bottom
                                 ;*	desc: stores string stored in program memory and writes it to the bottom line of the LCD screen on the right
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	REMEMBER: you must clear line outside of this function to prevent overwriting
                                 ;*	WARNING: assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_yz_bottom:
000137 930f                      push mpr
000138 93af                      push XL
000139 93bf                      push XH
00013a 93ef                      push ZL
00013b 93ff                      push ZH
00013c 93cf                      push YL
00013d 93df                      push YH
                                 
                                 ;must call lpm on Z, and need to call at end adress so shift Z->Y, and Y->Z for sake of function
00013e 2fae                      mov XL, ZL
00013f 2fbf                      mov XH, ZH ;X now temporarily holds old Z
                                 
000140 2fec                      mov ZL, YL
000141 2ffd                      mov ZH, YH ;Z now holds old Y 
                                 
000142 2fca                      mov YL, XL
000143 2fdb                      mov YH, XH ;Y now holds old Z via X
                                 
000144 e2a0                      ldi XL, LOW(lcd_buffer_addr+32) ;point x to the bottom line of the LCD buffer address in data memory
000145 e0b1                      ldi XH, HIGH(lcd_buffer_addr+32)
                                 
                                 print_yz_bottom_loop:
000146 9104                      lpm mpr, Z ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
000147 930c                      st X, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
000148 9731                      sbiw ZH:ZL, 1
000149 9711                      sbiw XH:XL, 1
                                 
                                 
00014a 17ec                      cp ZL, YL ;compare where Z points (beginning of string) to Y (Current address), we only need Low byte since start and end are definitely not far enough away to cause roll over errors
00014b f7d1                      brne print_yz_bottom_loop ;if not at end keep loading LCD buffer
                                 
00014c 9104                      lpm mpr, Z; store last character (dec YL inside loop triggers reset interrupt for some reason)
00014d 930c                      st X, mpr
                                 
00014e d076                      rcall	LCDWrLn2 ;once done write to LCD 
                                 
00014f 91df                      pop YH
000150 91cf                      pop YL
000151 91ff                      pop ZH
000152 91ef                      pop ZL
000153 91bf                      pop XH
000154 91af                      pop XL 
000155 910f                      pop mpr
000156 9508                      ret
                                 ;end print_yz_bottom
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
                                 
