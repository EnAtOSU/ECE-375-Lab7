                                 welcome_not_pressed:
0000b2 994f                      sbic PIND, PD7
0000b3 cffe                      rjmp welcome_not_pressed
                                 ;PD7 is now pressed
0000b4 0000                      nop
0000b5 0000                      nop ;avoid some debouncing
                                 welcome_pressed:
0000b6 9b4f                      sbis PIND, PD7
0000b7 cffe                      rjmp welcome_pressed
