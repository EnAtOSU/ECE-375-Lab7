                                 ;*****************************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*****************************************************************
                                 Bin2ASCII:
00014d 930f                      	push	 mpr			; save mpr
00014e 936f                      	push	 r			; save r
00014f 935f                      	push	 q			; save q
000150 93bf                      	push	 XH			; save X-pointer
000151 93af                      	push	 XL			;
                                 
                                 	; Determine the range of mpr
000152 3604                      	cpi	mpr, 100		; is mpr >= 100
000153 f018                      	brlo	 B2A_1			; goto next check
000154 e043                      	ldi	count, 3		; Three chars are written
000155 9613                      	adiw	 XL, 3			; Increment X 3 address spaces
000156 c007                      	rjmp	 B2A_3			; Continue with program
                                 B2A_1:
000157 300a                      	cpi	mpr, 10		; is mpr >= 10
000158 f018                      	brlo	 B2A_2			; Continue with program
000159 e042                      	ldi	count, 2		; Two chars are written
00015a 9612                      	adiw	 XL, 2			; Increment X 2 address spaces
00015b c002                      	rjmp	 B2A_3			; Continue with program
                                 B2A_2:
00015c 9611                      	adiw	 XL, 1			; Increment X 1 address space
00015d e041                      	ldi	count, 1		; One char is written
                                 
                                 B2A_3:
                                 	; Do-While statement that converts Binary to ASCII
00015e d076                      	rcall	div10			; Call the div10 function
00015f e300                      	ldi	mpr, '0'		; Set the base ASCII integer value
000160 0f06                      	add	mpr, r		; Create the ASCII integer value
000161 930e                      	st	-X, mpr		; Load ASCII value to memory
000162 2f05                      	mov	mpr, q		; Set mpr to quotient value
000163 3000                      	cpi	mpr, 0		; does mpr == 0
000164 f7c9                      	brne 	B2A_3			; do while (mpr != 0)
                                 
000165 91af                      	pop	XL			; restore X-pointer
000166 91bf                      	pop	XH			;
000167 915f                      	pop	q			; restore q
000168 916f                      	pop	r			; restore r
000169 910f                      	pop	mpr			; restore mpr
00016a 9508                      	ret				; return from function
                                 
