                                 ;*****************************************************************
                                 ;* Func:	LCDWrite
                                 ;* Desc:	This function writes 32 characters to the LCD screen...
                                 ;*		16 chars on the top line and 16 chars on the bottom line.
                                 ;*****************************************************************
                                 LCDWrite:
                                 	
000105 d002                      	rcall	LCDWrLn1
000106 d00c                      	rcall	LCDWrLn2
000107 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn1
                                 ;* Desc:	This function writes 16 characters to the top line.
                                 ;*****************************************************************
                                 LCDWrLn1:
000108 93bf                      	push	XH
000109 93af                      	push	XL
00010a 931f                      	push 	r17
                                 	; r17 specifies the destination bank in LCD RAM
00010b e013                      	ldi	r17, 3
                                 	; point X at the ASCII data
00010c e0b1                      	ldi	XH, HIGH(lcd_buffer_addr)
00010d e0a0                      	ldi	XL, LOW(lcd_buffer_addr)
00010e d05c                      	rcall	LCD_Internal_WriteLn
00010f 911f                      	pop	r17
000110 91af                      	pop	XL
000111 91bf                      	pop	XH
000112 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn2
                                 ;* Desc:	This function writes 16 characters to the bottom line.
                                 ;*****************************************************************
                                 LCDWrLn2:
000113 93bf                      	push	XH
000114 93af                      	push	XL
000115 931f                      	push	 r17
                                 	; r17 specifies the destination bank in LCD RAM
000116 e011                      	ldi	r17, 1
                                 	; point X at the ASCII data
000117 e0b1                      	ldi	XH, HIGH(lcd_buffer_addr+16)
000118 e1a0                      	ldi	XL, LOW(lcd_buffer_addr+16)
000119 d051                      	rcall	LCD_Internal_WriteLn
00011a 911f                      	pop	r17
00011b 91af                      	pop	XL
00011c 91bf                      	pop	XH
00011d 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDClr
                                 ;* Desc:	This function clears the entire LCD screen.
                                 ;*****************************************************************
                                 LCDClr:
00011e d022                      	rcall	LCDClrLn1
00011f d027                      	rcall	LCDClrLn2
                                 
000120 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	CLRDM1
                                 ;* Desc:	This function clears Data Memory by writing 16 
                                 ;*		consecutive ASCII spaces $20 to the memory at $0100.
                                 ;*****************************************************************
                                 CLRDM1:
                                     ; Set X register points to $0100
000121 93bf                      	   push	   XH
000122 93af                      	   PUSH	   XL
000123 930f                                 push    r16
000124 931f                                 push    r17
                                 
000125 e0b1                                 ldi     XH, HIGH(lcd_buffer_addr)
000126 e0a0                                 ldi     XL, LOW(lcd_buffer_addr)
000127 e110                                 ldi     r17, 16
000128 e200                                 ldi     r16, ' '
                                    CLRloop1:
000129 930d                                 st      X+, r16         ; load data to DM
00012a 951a                                 dec     r17
00012b f7e9                                 brne    CLRloop1
                                 
00012c 911f                      	   pop	   r17
00012d 910f                      	   pop	   r16
00012e 91af                      	   pop	   XL
00012f 91bf                      	   pop	   XH
                                 
000130 9508                                 ret
                                 
