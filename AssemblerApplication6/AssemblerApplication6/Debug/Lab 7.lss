000134 931f                                 push    r17
                                 
000135 e0b1                                 ldi     XH, HIGH(lcd_buffer_addr+16)
000136 e1a0                                 ldi     XL, LOW(lcd_buffer_addr+16)
000137 e110                                 ldi     r17, 16
000138 e200                                 ldi     r16, ' '
                                    CLRloop2:
000139 930d                                 st      X+, r16         ; load data to DM
00013a 951a                                 dec     r17
00013b f7e9                                 brne    CLRloop2
                                 
00013c 911f                      	   pop	   r17
00013d 910f                      	   pop	   r16
00013e 91af                      	   pop	   XL
00013f 91bf                      	   pop	   XH
                                 
000140 9508                                 ret
