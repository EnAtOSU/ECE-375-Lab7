                                 ;***********************************************************
                                 ;*	Func: print_zy_top
                                 ;*	desc: stores string stored in program memory and writes it to the top line of the LCD screen
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	WARNING - assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_top:
00007b 930f                      push mpr
00007c 93af                      push XL
00007d 93bf                      push XH
00007e 93ef                      push ZL
00007f 93ff                      push ZH
000080 93cf                      push YL
000081 93df                      push YH
                                 
000082 d0be                      rcall	LCDClrLn1 ;clear line to be writen to
000083 e0a0                      ldi XL, LOW(lcd_buffer_addr) ;point X to the top line of the LCD buffer address in data memory
000084 e0b1                      ldi XH, HIGH(lcd_buffer_addr)
                                 
                                 print_zy_top_loop:
000085 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
000086 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
000087 17ec                      cp ZL, YL  ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely far enough away to cause roll over errors
000088 f7e1                      brne print_zy_top_loop ;if not at end keep loading LCD buffer
                                 
000089 d07e                      rcall	LCDWrLn1 ;once done write to LCD
                                 
00008a 91df                      pop YH
00008b 91cf                      pop YL
00008c 91ff                      pop ZH
00008d 91ef                      pop ZL
00008e 91bf                      pop XH
00008f 91af                      pop XL 
000090 910f                      pop mpr
000091 9508                      ret
                                 ;end print_zy_bottom
                                 
                                 
                                 
                                 
                                 ;***********************************************************
                                 ;*	Func: print_zy_bottom
                                 ;*	desc: stores string stored in program memory and writes it to the top line of the LCD screen
                                 ;*	REMEMBER: the address stored in z and in y must be initially bit shifted by 1 due to least sig bit being low or high indicator. 
                                 ;*	WARNING - assumes Z stores the address of the beginning of the string and Y stores the end of the string to print.
                                 ;***********************************************************
                                 print_zy_bottom:
000092 930f                      push mpr
000093 93af                      push XL
000094 93bf                      push XH
000095 93ef                      push ZL
000096 93ff                      push ZH
000097 93cf                      push YL
000098 93df                      push YH
                                 
000099 d0ad                      rcall	LCDClrLn2 ;clear line to be writen to
00009a e1a0                      ldi XL, LOW(lcd_buffer_addr+16) ;point x to the bottom line of the LCD buffer address in data memory
00009b e0b1                      ldi XH, HIGH(lcd_buffer_addr+16)
                                 
                                 print_zy_bottom_loop:
00009c 9105                      lpm mpr, Z+ ;load value stored at the address to the beginning of the string (stored in X) to mpr, then inc X to point to next char. ie. first character of string is loaded into mpr
00009d 930d                      st X+, mpr ;Store that character to the beginning of the LCD buffer, then increment to next spot in LCD buffer
                                 
00009e 17ec                      cp ZL, YL ;compare where Z points (current address) to Y (end of string), we only need Low byte since start and end are definitely far enough away to cause roll over errors
00009f f7e1                      brne print_zy_bottom_loop ;if not at end keep loading LCD buffer
                                 
0000a0 d072                      rcall	LCDWrLn2 ;once done write to LCD 
                                 
0000a1 91df                      pop YH
0000a2 91cf                      pop YL
0000a3 91ff                      pop ZH
0000a4 91ef                      pop ZL
0000a5 91bf                      pop XH
0000a6 91af                      pop XL 
0000a7 910f                      pop mpr
0000a8 9508                      ret
                                 ;end print_zy_bottom
                                 
                                 
